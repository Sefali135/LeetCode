class Solution {
    public int lengthOfLongestSubstring(String s) {
        HashMap<Character,Integer> map= new HashMap<>();
        int j=0;int start=0;
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(map.containsKey(ch)){
                int in=map.get(ch);
             start=Math.max(start,in+1);
            }
              map.put(ch,i);
             j=Math.max(j, (i-start)+1);
        }
        return j;
    }
}
