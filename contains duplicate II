class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {
        HashMap<Integer,Integer> s=new HashMap<>();
       for(int i=0;i<nums.length;i++){
        int num=nums[i];
        if(s.containsKey(num)){
            int j=s.get(num);
            if((i-j)<=k){
                return true;
            }
       }
    s.put(num,i);
}
return false;
}
}
